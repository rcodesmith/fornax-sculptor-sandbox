<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
	<application>avega-supplier</application>
	<version>1</version>
	
	<system-properties>
		<!-- Configure java.util.logging -->
		<property name="java.util.logging.config.file" value="WEB-INF/logging.properties"/>
		<!-- Possibility to define appUrl, included in ServiceContext. 
		     Default is http://<appId>.appspot.com.
		     When running locally it is http://localhost:<port>
		-->
		<!-- 
		<property name="appUrl" value="http://myapp.foobar.com"/>
		-->
		
		<property name="supplier.name" value="Avega" />
	</system-properties>

	
</appengine-web-app>